[access]
	inheritFrom = All-Projects
[submit]
[project]
	description = Test repo for gerrit code review tool
[access "refs/*"]
	owner = group user/justinbraun3
[access "refs/for/refs/*"]
	label-Code-Review = -2..+2 group user/justinbraun3
	label-Verified = -1..+1 group user/justinbraun3
	push = group user/justinbraun3
	read = group user/justinbraun3
	rebase = group user/justinbraun3
	removeReviewer = group user/justinbraun3
	submit = group user/justinbraun3
[access "refs/heads/*"]
	create = group user/justinbraun3
	push = +force group user/justinbraun3
	pushMerge = group user/justinbraun3
	read = group user/justinbraun3
[access "refs/tags/*"]
	push = group user/justinbraun3
	push = +force group user/justinbraun3
